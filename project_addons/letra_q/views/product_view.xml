<odoo>

    <record id="view_template_letra_q_form" model="ir.ui.view">
        <field name="name">Product Letra Q</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="stock.view_template_property_form"/>
        <field name="arch" type="xml">
            <field name="tracking" position="after">
                <field name="subject_q" attrs="{'invisible':[('tracking', '=', 'none')]}"/>
                <field name="species_q" />
            </field>
        </field>
    </record>

</odoo>